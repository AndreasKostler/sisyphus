  Doi                      = {10.1162/089976603321192121},
  File                     = {:papers/Hurri2003.pdf:PDF},
  ISSN                     = {0899-7667},
  Keywords                 = {ica, slowfeatureanalysis, slowness\_definition, sparse-coding, video},
