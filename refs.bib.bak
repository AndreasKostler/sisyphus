@Article{Lee1998,
  Title                    = {The role of the primary visual cortex in higher level vision},
  Author                   = {Tai Sing Lee and David Mumford and Richard Romero and Victor A.F. Lamme},
  Journal                  = {Vision Research },
  Year                     = {1998},
  Number                   = {15–16},
  Pages                    = {2429--2454},
  Volume                   = {38},

  Abstract                 = {In the classical feed-forward, modular view of visual processing, the primary visual cortex (area V1) is a module that serves to extract local features such as edges and bars. Representation and recognition of objects are thought to be functions of higher extrastriate cortical areas. This paper presents neurophysiological data that show the later part of \{V1\} neurons{\textquoteright} responses reflecting higher order perceptual computations related to Ullman{\textquoteright}s (Cognition 1984;18:97--159) visual routines and Marr{\textquoteright}s (Vision NJ: Freeman 1982) full primal sketch, 212D sketch and 3D model. Based on theoretical reasoning and the experimental evidence, we propose a possible reinterpretation of the functional role of V1. In this framework, because of \{V1\} neurons{\textquoteright} precise encoding of orientation and spatial information, higher level perceptual computations and representations that involve high resolution details, fine geometry and spatial precision would necessarily involve \{V1\} and be reflected in the later part of its neurons{\textquoteright} activities. },
  Doi                      = {10.1016/S0042-6989(97)00464-1},
  File                     = {:papers/Lee1998.pdf:PDF},
  ISSN                     = {0042-6989},
  Keywords                 = {Figure-ground segregation},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0042698997004641}
}

